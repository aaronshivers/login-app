<% include partials/top %>

<div class="container">

  <h1 class="text-center">Login Form</h1>

  <form class="form" action="/login" method="POST">
    
    <div class="form-group">
      <label for="email">Email</label>
      <input class="form-control" type="text" name="email" placeholder="Enter Email..." "autocomplete="email" required>
    </div>

    <div class="form-group">
      <label for="password">Email</label>
      <input class="form-control" type="text" name="password" placeholder="Enter Password..." "autocomplete="password" required>
    </div>

    <button class="btn btn-outline-primary btn-block">Login</button>

  </form>

  <div id="test"></div>

</div>

